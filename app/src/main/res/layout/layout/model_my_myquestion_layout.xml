<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <TextView
        android:id="@+id/model_my_myquestion_icon"
        android:layout_width="@dimen/dp_7"
        android:layout_height="@dimen/dp_7"
        android:layout_marginLeft="@dimen/dp13"
        android:layout_marginTop="@dimen/dp22"
        android:background="@drawable/circle_style_orange"
        />
    <TextView
        android:id="@+id/model_my_myquestion_content"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginLeft="@dimen/dp13"
        android:layout_marginRight="@dimen/dp13"
        android:layout_marginTop="@dimen/dp15"
        android:text=""
        android:textColor="#ff000000"
        android:textSize="@dimen/textsize16"
        android:layout_toRightOf="@id/model_my_myquestion_icon"
        />
    <View
        android:id="@+id/model_my_myquestion_line"
        android:layout_below="@id/model_my_myquestion_content"
        android:layout_width="match_parent"
        android:layout_height="@dimen/dp1"
        android:layout_marginLeft="@dimen/dp13"
        android:layout_marginTop="@dimen/dp13"
        android:layout_marginRight="@dimen/dp13"
        android:background="#E5E5E5"/>
</RelativeLayout>