<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
<item android:state_pressed="false" android:state_enabled="true">
    <shape android:shape="rectangle" >
        <solid android:color="#FF5A30" />
        <!-- 设置按钮的四个角为弧形 -->
          <!-- android:radius 弧形的半径 -->
          <corners android:topLeftRadius="@dimen/dp23"
        android:topRightRadius="@dimen/dp23"
        android:bottomRightRadius="@dimen/dp23"
        android:bottomLeftRadius="@dimen/dp23"/>
<!--        <corners android:radius="5dp" />-->
<!--        <stroke android:width="1dp" android:color="#acacac" />-->
    </shape>
</item>

<item android:state_pressed="true" android:state_enabled="true">
    <shape android:shape="rectangle">
        <solid android:color="#FF5A30" />
        <!-- 设置按钮的四个角为弧形 -->
          <!-- android:radius 弧形的半径 -->
          <corners android:topLeftRadius="@dimen/dp23"
        android:topRightRadius="@dimen/dp23"
        android:bottomRightRadius="@dimen/dp23"
        android:bottomLeftRadius="@dimen/dp23"/>
<!--        <corners android:radius="5dp" />-->
<!--        <stroke android:width="1dp" android:color="#acacac" />-->
    </shape>
</item>

    <item  android:state_enabled="false">
        <shape android:shape="rectangle">
            <solid android:color="#FFCFC3" />
            <!-- 设置按钮的四个角为弧形 -->
              <!-- android:radius 弧形的半径 -->
              <corners android:topLeftRadius="@dimen/dp23"
            android:topRightRadius="@dimen/dp23"
            android:bottomRightRadius="@dimen/dp23"
            android:bottomLeftRadius="@dimen/dp23"/>
            <!--        <corners android:radius="5dp" />-->
            <!--        <stroke android:width="1dp" android:color="#acacac" />-->
        </shape>
    </item>

</selector>
